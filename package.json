{
  "name": "@worktile/planet",
  "version": "1.0.7",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run serve:portal\" \"npm run serve:app1\" \"npm run serve:app2\"",
    "serve:portal": "ng run portal:serve",
    "serve:app1": "ng serve app1",
    "serve:app2": "ng serve app2",
    "build:portal": "ng build",
    "build:app1": "ng build app1",
    "build:app2": "ng build app2",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --pkg ./packages/planet/package.json",
    "build": "ng build planet",
    "test": "ng test planet",
    "lint": "ng lint planet",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "e2e": "ng e2e",
    "pub-only": "cd dist/planet && npm publish --access=public",
    "pub": "npm run build && npm run pub-only"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/cdk": "^7.2.1",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "angular-sortablejs": "^2.6.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "ngx-bootstrap": "3.0.0",
    "ngx-tethys": "^7.5.20",
    "rxjs": "~6.4.0",
    "sortablejs": "^1.8.0-rc1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^7.3.1",
    "@angular-builders/dev-server": "^7.3.1",
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.11.0",
    "@angular/cli": "~7.3.4",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "conventional-changelog-cli": "^2.0.12",
    "coveralls": "^3.0.6",
    "css-loader": "^2.1.0",
    "husky": "^2.4.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mini-css-extract-plugin": "^0.5.0",
    "ng-packagr": "^4.2.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "protractor": "~5.4.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.10.0",
    "tslint": "~5.11.0",
    "typescript": "3.1.6",
    "webpack-assets-manifest": "^3.1.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  }
}
