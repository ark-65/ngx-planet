                    }
                },
                "serve1": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "app1:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "app1:build:production"
