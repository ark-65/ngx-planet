                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "examples/app1/src/test.ts",
                        "polyfills": "examples/app1/src/polyfills.ts",
                        "tsConfig": "examples/app1/tsconfig.spec.json",
